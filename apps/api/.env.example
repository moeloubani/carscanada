# Database
DATABASE_URL="postgresql://user:password@localhost:5432/carscanada?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# Server
PORT=3001
NODE_ENV=development

# JWT
JWT_SECRET=your-jwt-secret-here
JWT_REFRESH_SECRET=your-jwt-refresh-secret-here
JWT_EXPIRE_TIME=15m
JWT_REFRESH_EXPIRE_TIME=7d

# AWS S3
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_S3_BUCKET=carscanada-images

# Stripe
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Email (SendGrid)
SENDGRID_API_KEY=
EMAIL_FROM=noreply@carscanada.ca

# Frontend URLs
FRONTEND_URL=http://localhost:3000
MOBILE_SCHEME=carscanada://

# Socket.io
SOCKET_IO_SECRET=your-socket-secret-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100